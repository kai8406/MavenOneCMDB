	<!-- NMAP Transform information start -->
		<DataSet alias="nmap-transform">
			<instanceSelector>
				<ref>
					<ForwardInstanceSelector alias="nmap-fw-1">
						<name>Nmap</name>
					</ForwardInstanceSelector>
				</ref>
			</instanceSelector>
			
			<attributeSelector>
				<ref>
					<ComplexAttributeSelector>
						<name>os-folder</name>
						<dataSet>
							<ref alias="os-folder-discovery" />
						</dataSet>
					</ComplexAttributeSelector>
				</ref>
			</attributeSelector>
			<attributeSelector>
				<ref>
					<ComplexAttributeSelector>
						<name>software-folder</name>
						<dataSet>
							<ref alias="software-folder-discovery" />
						</dataSet>
					</ComplexAttributeSelector>
				</ref>
			</attributeSelector>
			<attributeSelector>
				<ref>
					<ComplexAttributeSelector>
						<name>hardware-folder</name>
						<dataSet>
							<ref alias="hardware-folder-discovery" />
						</dataSet>
					</ComplexAttributeSelector>
				</ref>
			</attributeSelector>
				
		</DataSet>
		
		<DataSet alias="os-folder-discovery">
			<instanceSelector>
				<ref>
					<XPathInstanceSelector>
						<name>os folder</name>
						<xpath>/*</xpath>
						<template>OSFolder</template>
						<instance>OSFolder1</instance>
					</XPathInstanceSelector>
				</ref>
			</instanceSelector>
			
			<attributeSelector>
				<ref>
					<XPathComplexAttributeSelector>
						<name>OS</name>
						<dataSet>
							<ref alias="nmap-os" />
						</dataSet>
						<xpath>//host/os</xpath>
					</XPathComplexAttributeSelector>
				</ref>
			</attributeSelector>
		</DataSet>
		
		<DataSet alias="software-folder-discovery">
			<instanceSelector>
				<ref>
					<XPathInstanceSelector>
						<name>software folder</name>
						<xpath>/*</xpath>
						<template>SoftwareFolder</template>
						<instance>SoftwareFolder1</instance>
					</XPathInstanceSelector>
				</ref>
			</instanceSelector>
			
			<attributeSelector>
				<ref>
					<XPathComplexAttributeSelector>
						<name>Software</name>
						<dataSet>
							<ref alias="nmap-software" />
						</dataSet>
						<xpath>//host/ports/port[state[@state='open']]</xpath>
					</XPathComplexAttributeSelector>
				</ref>
			</attributeSelector>
		</DataSet>
		
		<DataSet alias="hardware-folder-discovery">
			<instanceSelector>
				<ref>
					<XPathInstanceSelector>
						<name>hardware folder</name>
						<xpath>/*</xpath>
						<template>HardwareFolder</template>
						<instance>HardwareFolder1</instance>
					</XPathInstanceSelector>
				</ref>
			</instanceSelector>
			
			<attributeSelector>
				<ref>
					<ComplexAttributeSelector>
						<name>Hardwares</name>
						<dataSet>
							<ref alias="nmap-hardware" />
						</dataSet>
					</ComplexAttributeSelector>
				</ref>
			</attributeSelector>
		</DataSet>
	
		<DataSet alias="nmap-hardware">
			<instanceSelector>
				<ref>
					<XPathInstanceSelector>
						<name>hosts</name>
						<xpath>//host[status[@state='up']]</xpath>
						<template>Hardware</template>
					</XPathInstanceSelector>
				</ref>
			</instanceSelector>
			<attributeSelector>
				<ref>
					<XPathAttributeSelector>
						<name>L_IP_Address</name>
						<xpath>address[@addrtype='ipv4']/@addr</xpath>
						<naturalKey>true</naturalKey>
					</XPathAttributeSelector>
				</ref>
			</attributeSelector>
		
			<attributeSelector>
				<ref>
					<XPathAttributeSelector>
						<name>M_Hostname</name>
						<xpath>hostnames/hostname/@name</xpath>
						<naturalKey>false</naturalKey>
					</XPathAttributeSelector>
				</ref>
			</attributeSelector>

			<attributeSelector>
				<ref>
					<XPathAttributeSelector>
						<name>L_MAC_Address</name>
						<xpath>address[@addrtype='mac']/@addr</xpath>
					</XPathAttributeSelector>
				</ref>
			</attributeSelector>

			<attributeSelector>
				<ref>
					<ComplexAttributeSelector>
						<name>K_Software</name>
						<dataSet>
							<ref alias="nmap-software" />
						</dataSet>
					</ComplexAttributeSelector>
				</ref>
			</attributeSelector>

			<attributeSelector>
				<ref>
					<ComplexAttributeSelector>
						<name>J_OS</name>
						<dataSet>
							<ref alias="nmap-os" />
						</dataSet>
					</ComplexAttributeSelector>
				</ref>
			</attributeSelector>


		</DataSet>

		<DataSet alias="nmap-software">
			<instanceSelector>
				<ref>
					<XPathInstanceSelector>
						<name>Software</name>
						<xpath>ports/port[state[@state='open']]</xpath>
						<template>Software</template>
					</XPathInstanceSelector>
				</ref>
			</instanceSelector>
			<attributeSelector>
				<ref>
					<XPathAttributeSelector>
						<name>C_Port</name>
						<xpath>@portid</xpath>
					</XPathAttributeSelector>
				</ref>
			</attributeSelector>
			<attributeSelector>
				<ref>
					<XPathAttributeSelector>
						<name>C_Protocol</name>
						<xpath>@protocol</xpath>
					</XPathAttributeSelector>
				</ref>
			</attributeSelector>

			<attributeSelector>
				<ref>
					<XPathAttributeSelector>
						<name>A_Name</name>
						<xpath>service/@name</xpath>
						<naturalKey>true</naturalKey>
					</XPathAttributeSelector>
				</ref>
			</attributeSelector>
		</DataSet>

		<DataSet alias="nmap-os">
			<instanceSelector>
				<ref>
					<XPathInstanceSelector>
						<name>os</name>
						<xpath>os</xpath>
						<template>OS</template>
					</XPathInstanceSelector>
				</ref>
			</instanceSelector>

			<attributeSelector>
				<ref>
					<XPathAttributeSelector>
						<name>A_Name</name>
						<xpath>osmatch/@name</xpath>
						<naturalKey>true</naturalKey>
					</XPathAttributeSelector>
				</ref>
			</attributeSelector>

			<attributeSelector>
				<ref>
					<XPathAttributeSelector>
						<name>C_Vendor</name>
						<xpath>osclass/@vendor</xpath>
						<naturalKey>true</naturalKey>
					</XPathAttributeSelector>
				</ref>
			</attributeSelector>

			<attributeSelector>
				<ref>
					<XPathAttributeSelector>
						<name>C_Family</name>
						<xpath>osclass/@osfamily</xpath>
						<naturalKey>true</naturalKey>
					</XPathAttributeSelector>
				</ref>
			</attributeSelector>
		</DataSet>
		<!-- NMAP Transform information end -->
</instances>
