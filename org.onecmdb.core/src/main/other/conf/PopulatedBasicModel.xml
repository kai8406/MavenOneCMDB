<?xml version="1.0" encoding="iso-8859-1"?>
<onecmdb>
<!-- Built in templates and instances are defines in XML files in subdirectories of org.onecmdb.core.internal -->  
<templates>
  <!-- Common templates -->
	<template alias="Folder" displayName="Folder">
		<derivedFrom>
			<ref alias="Ci"/>
		</derivedFrom>
		<description>This is the master template for your folders.</description>
		<setSimpleValue attAlias="icon">folder</setSimpleValue>
	</template>
	
	<template alias="Resource" displayName="Resource">
		<derivedFrom>
			<ref alias="Ci"/>
		</derivedFrom>
		<description>This is the master template for your data center templates.</description>
		<setSimpleValue attAlias="icon">resource</setSimpleValue>
	</template>
	
	<template alias="MyDataCenterRef" displayName="MyDataCenterRef">
		<derivedFrom>
			<ref alias="Reference"/>
		</derivedFrom>
		<description>This is the master template for your references.</description>
		<setSimpleValue attAlias="icon">reference</setSimpleValue>
	</template>
	
	<template alias="Uses" displayName="Uses">
		<derivedFrom>
			<ref alias="MyDataCenterRef"/>
		</derivedFrom>
		<description>This is a general CI dependency reference.</description>
		<setSimpleValue attAlias="icon">reference</setSimpleValue>
	</template>
	
	<template alias="ImportModel" displayName="ImportModel">
		<derivedFrom>
			<ref alias="JobProcess"/>
		</derivedFrom>
		<description>This job imports configuration items from an XML file.</description>
		<attribute attAlias="importUrl">
			<simpleType>xs:string</simpleType>
			<policy>
				<maxOccurs>unbound</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<setSimpleValue attAlias="icon">job</setSimpleValue>
		<setSimpleValue attAlias="javaClass">org.onecmdb.core.internal.job.workflow.sample.UrlImportProcess</setSimpleValue>
	</template>

	<template alias="ExportModel" displayName="ExportModel">
		<derivedFrom>
			<ref alias="JobProcess"/>
		</derivedFrom>
		<description>This job exports configuration items to an XML file.</description>
		<attribute attAlias="instances">
			<description>Export Instances if set.</description>
			<simpleType>xs:boolean</simpleType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<attribute attAlias="exportFile">
			<simpleType>xs:string</simpleType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<attribute attAlias="rootAlias">
			<description>Export all CIs based on this template (enter alias). Will export this and descendants according to
			the templates/instances attributes.</description>
			<simpleType>xs:string</simpleType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<attribute attAlias="templates">
			<description>Export Templates if set.</description>
			<simpleType>xs:boolean</simpleType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<setSimpleValue attAlias="icon">job</setSimpleValue>
		<setSimpleValue attAlias="javaClass">org.onecmdb.core.internal.job.workflow.sample.ExportModelProcess</setSimpleValue>
	</template>
	
  <!-- Basic templates -->
	<template alias="OS" displayName="${A_Name}">
		<derivedFrom>
			<ref alias="Resource"/>
		</derivedFrom>
		<description>This CI defines one Operating System</description>
		<attribute attAlias="F_Financial_Info" displayName="Financial Info">
			<description>Financial info about this CI</description>
			<complexType>
				<ref alias="Financials"/>
				<refType><ref alias="Uses"/></refType>
			</complexType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<attribute attAlias="A_Name" displayName="Name">
			<description>Name of this software</description>
			<simpleType>xs:string</simpleType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<attribute attAlias="D_DSL_Entry" displayName="DSL Entry">
			<description>Link to the correct software package in the DSL</description>
			<simpleType>xs:anyURI</simpleType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<attribute attAlias="C_Vendor" displayName="Vendor">
			<description>The vendor name for this OS.</description>
			<simpleType>xs:string</simpleType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<attribute attAlias="C_Family" displayName="OS Family">
			<description>The family name for this OS.</description>
			<simpleType>xs:string</simpleType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		
		<attribute attAlias="C_Target_CPU" displayName="Target CPU">
			<description>CPU type this software is aimed for</description>
			<complexType>
				<ref alias="CPU"/>
				<refType><ref alias="Uses"/></refType>
			</complexType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<attribute attAlias="E_Maintenance_Info" displayName="Maintenance Info">
			<description>Maintenance info for this software</description>
			<complexType>
				<ref alias="Maintenance"/>
				<refType><ref alias="Uses"/></refType>
			</complexType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<attribute attAlias="B_Version" displayName="Version">
			<description>Version number for this software</description>
			<simpleType>xs:string</simpleType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<setSimpleValue attAlias="icon">package</setSimpleValue>
		<setSimpleValue attAlias="A_Name">OS</setSimpleValue>
	</template>
	
	<template alias="Location" displayName="${A_Name}">
		<derivedFrom>
			<ref alias="Resource"/>
		</derivedFrom>
		<description>This CI specifies a data center location.</description>
		<attribute attAlias="A_Name" displayName="Name">
			<description>Name of this location</description>
			<simpleType>xs:string</simpleType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<setSimpleValue attAlias="icon">resource</setSimpleValue>
		<setSimpleValue attAlias="A_Name">Location</setSimpleValue>
	</template>
	<template alias="HardwareType" displayName="${A_Name}">
		<derivedFrom>
			<ref alias="Resource"/>
		</derivedFrom>
		<description>This CI specifies the type of hardware.</description>
		<attribute attAlias="A_Name" displayName="Name">
			<description>The name of this hardware type</description>
			<simpleType>xs:string</simpleType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<setSimpleValue attAlias="A_Name">Hardware Type</setSimpleValue>
	</template>
	
	<template alias="Hardware" displayName="${A_Name} ${L_IP_Address}">
		<derivedFrom>
			<ref alias="Resource"/>
		</derivedFrom>
		<description>This CI specifies a hardware that has a network connection.</description>
		<attribute attAlias="F_Serial_Number" displayName="Serial Number">
			<description>Serial number for this specific hardware</description>
			<simpleType>xs:string</simpleType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<attribute attAlias="M_Hostname" displayName="Hostname">
			<description>Hostname associated with this hardware</description>
			<simpleType>xs:string</simpleType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<attribute attAlias="K_Software" displayName="Software">
			<description>Installed software packages on this hardware</description>
			<complexType>
				<ref alias="Software"/>
				<refType><ref alias="Uses"/></refType>
			</complexType>
			<policy>
				<maxOccurs>unbound</maxOccurs>
				<minOccurs>0</minOccurs>
			</policy>
		</attribute>
		<attribute attAlias="A_Type" displayName="Type">
			<description>The type of this hardware.</description>
			<complexType>
				<ref alias="HardwareType"/>
				<refType><ref alias="Uses"/></refType>
			</complexType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		
		<attribute attAlias="G_CPU_Type" displayName="CPU Type">
			<description>Type of CPU in this hardware</description>
			<complexType>
				<ref alias="CPU"/>
				<refType><ref alias="Uses"/></refType>
			</complexType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<attribute attAlias="N_Maintenance_Info" displayName="Maintenance Info">
			<description>Maintenance info for this hardware</description>
			<complexType>
				<ref alias="Maintenance"/>
				<refType><ref alias="Uses"/></refType>
			</complexType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<attribute attAlias="A_Name" displayName="Name">
			<description>The name of this hardware</description>
			<simpleType>xs:string</simpleType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<attribute attAlias="B_Location" displayName="Location">
			<description>Data center where this hardware is located</description>
			<complexType>
				<ref alias="Location"/>
				<refType><ref alias="Uses"/></refType>
			</complexType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<attribute attAlias="D_Operational_Status" displayName="Operational Status">
			<description>Select between Not installed/Under test/In production</description>
			<simpleType>xs:string</simpleType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<attribute attAlias="E_Manufacturer" displayName="Manufacturer">
			<description>The manufacturer of this </description>
			<complexType>
				<ref alias="Company"/>
				<refType><ref alias="Uses"/></refType>
			</complexType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<attribute attAlias="L_IP_Address" displayName="IP Address">
			<description>IP Address associated with this </description>
			<simpleType>xs:string</simpleType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<attribute attAlias="L_MAC_Address" displayName="MAC Address">
			<description>MAC Address associated with this </description>
			<simpleType>xs:string</simpleType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>

		<attribute attAlias="H_RAM_Size" displayName="RAM Size (MB)">
			<description>Size of RAM memory in megabytes (MB)</description>
			<simpleType>xs:integer</simpleType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<attribute attAlias="J_OS" displayName="OS">
			<description>Operating system installed on this </description>
			<complexType>
				<ref alias="OS"/>
				<refType><ref alias="Uses"/></refType>
			</complexType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<attribute attAlias="O_Financial_Info" displayName="Financial Info">
			<description>Financial info for this </description>
			<complexType>
				<ref alias="Financials"/>
				<refType><ref alias="Uses"/></refType>
			</complexType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<attribute attAlias="C_Used_By" displayName="Used By">
			<description>Customer using this </description>
			<complexType>
				<ref alias="Company"/>
				<refType><ref alias="Uses"/></refType>
			</complexType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<attribute attAlias="I_HD_Size" displayName="HD Size (GB)">
			<description>Size of hard disk in gigabytes (GB)</description>
			<simpleType>xs:integer</simpleType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<setSimpleValue attAlias="icon"></setSimpleValue>
		<setSimpleValue attAlias="A_Name">Hardware</setSimpleValue>
	</template>
	
  <template alias="Financials" displayName="Financials">
		<derivedFrom>
			<ref alias="Resource"/>
		</derivedFrom>
		<description>This CI contains financial information for one or several pieces of equipment or software packages.</description>
		<attribute attAlias="B_Purchase_Date" displayName="Purchase Date">
			<description>Date when this item was purchased</description>
			<simpleType>xs:date</simpleType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<attribute attAlias="E_Purchase_Contract" displayName="Purchase Contract">
			<description>Link to the purchase contract document</description>
			<simpleType>xs:anyURI</simpleType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<attribute attAlias="D_Maintenance_Fee" displayName="Maintenance Fee (USD/year)">
			<description>The fee for maintenance in USD per year</description>
			<simpleType>xs:integer</simpleType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<attribute attAlias="C_Depreciation" displayName="Depreciation (years)">
			<description>Depreciation expressed in number of years</description>
			<simpleType>xs:integer</simpleType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<attribute attAlias="A_Purchase_Price" displayName="Purchase Price (USD)">
			<description>The purchase price in USD for this item</description>
			<simpleType>xs:integer</simpleType>
			<policy>
				<maxOccurs>1</maxOccurs>
				<minOccurs>1</minOccurs>
			</policy>
		</attribute>
		<setSimpleValue attAlias="icon">asset</setSimpleValue>
	</template>
	
	<template alias="CPU" displayName="${A_Name}">
